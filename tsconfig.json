{
  "extends": "./node_modules/expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  "module": "NodeNext",
  "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "**/*.js"
  ]
}
